 int j;

            for (i = 0; i < vector.Length; i++)
            {
                for (j = 0; j < vector.Length; j++)
                {
                    if (vector[i] < vector[j])
                    {
                        int temp = vector[i]; 
                        vector[i] = vector[j];
                        vector[j] = temp;
                    }
                }
            }
